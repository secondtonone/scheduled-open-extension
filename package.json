{
  "name": "scheduled-open-extension",
  "version": "1.0.5",
  "description": "ChronoLinkOpener extension for opening pages by scheduler",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --color profile",
    "dev": "NODE_ENV=development webpack --progress --color profile",
